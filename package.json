{
  "name": "shopify_storefront_sdk",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "ts-node index.ts",
    "build": "tsc",
    "dev:test": "ts-node src/tests/test.ts",
    "test": "node dist/tests/test.js",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [
    "shopify",
    "shopify storefront",
    "graphql",
    "ecommerce",
    "sdk",
    "typescript",
    "shopify-sdk",
    "graphql-request",
    "storefront-api"
  ],
  "author": "Paschal Elechi",
  "license": "MIT",
  "description": "A fully typed Node.js + TypeScript SDK for the Shopify Storefront GraphQL API.",
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@types/node": "^24.10.1",
    "axios": "^1.13.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0"
  }
}
